{
  "name": "React Chrome Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "action": {
    "default_title": "Chrome Extension Boilerplate",
    "default_popup": "./public/index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}